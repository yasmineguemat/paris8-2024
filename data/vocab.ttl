@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix res: <https://example.org/reservation#> .

<https://example.org/reservation> a owl:Ontology ;
    dcterms:title "Vocabulaire de Réservation" .

res:Reservation a rdfs:Class ;
    rdfs:label "Réservation" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

res:nomComplet a rdf:Property ;
    rdfs:label "Nom complet" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:adresseEmail a rdf:Property ;
    rdfs:label "Adresse e-mail" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:numeroTelephone a rdf:Property ;
    rdfs:label "Numéro de téléphone" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:serviceReserver a rdf:Property ;
    rdfs:label "Service ou ressource réservée" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:creneauHoraire a rdf:Property ;
    rdfs:label "Créneau horaire" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:durée a rdf:Property ;
    rdfs:label "Durée" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:objectifDescription a rdf:Property ;
    rdfs:label "Objectif ou description" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .

res:nombrePersonnes a rdf:Property ;
    rdfs:label "Nombre de personnes présentes" ;
    rdfs:domain res:Reservation ;
    vs:term_status "experimental" .